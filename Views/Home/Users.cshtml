@{
    ViewData["Title"] = "Users";
}
<h1>@ViewData["Title"]</h1>

<table class="table table-responsive table-hover">
    <tr>

        <th>id</th>
        <th>name</th>
        <th>Family</th>
        <th>Fathername</th>
        <th>Password</th>
        <th>Birthday</th>
        <th>Gender</th>
        <th>Active</th>
                
    </tr>
@{
    foreach(var item in Model){
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Family</td>
            <td>@item.Fathername</td>
            <td>@item.Password</td>
            <td>@item.Birthday</td>
            <td>@item.Gender</td>
            <td>@item.Active</td>
            
        </tr>
    }
}
</table>